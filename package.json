{
  "name": "babel-plugin-module-rewrite",
  "version": "1.0.0",
  "main": "lib/index.js",
  "description": "Babel plugin to dynamically rewrite the path in require() and ES6 import",
  "repository": {
    "type": "git",
    "url": "https://github.com/liady/babel-plugin-module-rewrite.git"
  },
  "author": {
    "name": "Liad Yosef"
  },
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "module",
    "rewrite",
    "resolve",
    "dynamic",
    "rename",
    "require",
    "import"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "cross-env": "^1.0.8",
    "mocha": "^2.5.0"
  },
  "scripts": {
    "compile": "cross-env NODE_ENV=production babel src --out-dir lib",
